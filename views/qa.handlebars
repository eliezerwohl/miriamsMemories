
<div class="col-md-8">
{{#each results}}
<div class="panel panel-default">
  <div class="panel-body">
    <div class="col-md-8">
      <h3>{{this.question}}</h3>
      <p>{{this.answer}}</p>
      <br>
    </div>
    <div class="col-md-4">
      <a href="/ViewNote/{{this.id}}">
        <button class="btn btn-default btn-lg btn-block btn-default">View Notes </button>
<!--         view notes goes to a page which display the question and answer, add can add a note -->
      </a>
    </div>
  </div>
</div>

{{/each}}
  </div>
<div class="col-md-4">
<a href="/showAll"><div class="col-md-12 btnDiv">
    <button class="btn btn-default btn-lg btn-block">Back</button>
  </div></a>
  </div>

